{
    "name": "pasanks/str-converter",
    "description": "PHP string manipulator",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Pasan Kavinga"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.0",
        "symfony/process": "^6.0",
        "illuminate/support": "^9.0",
        "symfony/var-dumper": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5"
    },
    "bin": [
        "bin/converter"
    ],
    "autoload": {
        "psr-4": {
            "Pasanks\\StrConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pasanks\\StrConverter\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
